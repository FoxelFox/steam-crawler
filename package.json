{
  "name": "steam-crawler",
  "version": "0.0.1",
  "description": "crawls stuff from steam",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "crawl-index": "npm run build && node dist/crawl-index.js",
    "crawl-images": "npm run build && node dist/crawl-images.js header",
    "more-like-this": "npm run build && node dist/more-like-this.js",
    "items": "npm run build && node dist/items.js",
    "ssim": "npm run build && node dist/ssim.js",
    "tensorflow": "npm run build && node dist/tensorflow.js",
    "resize": "npm run build && node dist/resize.js 2",
    "crawl-all-continue": "npm run build && node dist/crawl-index.js && node dist/crawl-images.js header && node dist/resize.js 128 && node dist/more-like-this.js && node dist/items.js",
    "crawl-all": "npm run build && node dist/crawl-index.js force && node dist/crawl-images.js header && node dist/resize.js 128 && node dist/more-like-this.js && node dist/items.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoxelFox/steam-crawler.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FoxelFox/steam-crawler/issues"
  },
  "homepage": "https://github.com/FoxelFox/steam-crawler#readme",
  "devDependencies": {
    "@types/node": "^12.0.4",
    "clear": "^0.1.0",
    "image-ssim": "^0.2.0",
    "jpeg-js": "^0.3.5",
    "node-vibrant": "^3.2.0-alpha",
    "request": "^2.88.0",
    "sharp": "^0.22.1",
    "typescript": "^3.5.1"
  }
}
